# Copy this file to terraform.tfvars and fill in your values

# Your AWS EC2 key pair name (must already exist in us-east-1)
ssh_key_name = "your-key-name"

# Your IP address for SSH access (use https://whatismyip.com)
# Format: "1.2.3.4/32" (the /32 means only your specific IP)
your_ip = "0.0.0.0/32"

# Optional: Override defaults
# instance_type = "t3.medium"
# project_name  = "catalyst-server"
